<project>
  <actions/>
  <description></description>
  <project-properties class="concurrent-hash-map">
    <entry>
      <string>builders</string>
      <describable-list-property>
        <originalValue class="hudson.util.DescribableList">
          <hudson.tasks.Shell>
            <disabled>false</disabled>
            <description>Clone git repositories </description>
            <command>git clone https://github.com/${FORK}/bridgepoint.git --branch ${BRANCH} --depth 1
git clone https://github.com/${FORK}/mc.git --branch ${BRANCH} --depth 1
git clone https://github.com/${FORK}/packaging.git --branch ${BRANCH} --depth 1
git clone https://github.com/${FORK}/pt_antlr.git --branch ${BRANCH} --depth 1</command>
          </hudson.tasks.Shell>
          <hudson.tasks.Shell>
            <disabled>false</disabled>
            <description>Prebuild step</description>
            <command># Update the build ID
curdate=$(TZ=&apos;America/New_York&apos; date +&quot;%F %R&quot;)
sed -i &quot;s/InternalBuild/$curdate/&quot; bridgepoint/src/org.xtuml.bp.pkg/about.mappings
mkdir -p /tmp/buildinfo
cp bridgepoint/src/org.xtuml.bp.pkg/about.mappings /tmp/buildinfo

# copy preferences
mkdir -p wse/.metadata/.plugins/org.eclipse.core.runtime/.settings
cp -f bridgepoint/utilities/build/preferences/* wse/.metadata/.plugins/org.eclipse.core.runtime/.settings

#AntLr
cp -f pt_antlr/pt_antlr/antlr.jar bridgepoint/src/org.xtuml.bp.als/lib/antlr.jar

# generation
# import to workspace
/build/buildmt/BridgePoint/Launcher.sh -nosplash -data wse -application org.eclipse.cdt.managedbuilder.core.headlessbuild -importAll bridgepoint/src

# generate
# 
/build/buildmt/BridgePoint/CLI.sh Build -project org.xtuml.bp.core -prebuildOnly
/build/buildmt/BridgePoint/CLI.sh Build -project org.xtuml.bp.als -prebuildOnly
/build/buildmt/BridgePoint/CLI.sh Build -project org.xtuml.bp.ui.canvas -prebuildOnly</command>
          </hudson.tasks.Shell>
          <hudson.tasks.Shell>
            <disabled>false</disabled>
            <description>Unit Test - Prepare the environment for unit testing</description>
            <command>#!/bin/bash
if [ &quot;$INCLUDE_TESTS&quot; = &quot;true&quot; ]; then
  git clone https://github.com/${FORK}/bptest --branch ${BRANCH} --depth 1
  git clone https://github.com/${FORK}/models.git --branch ${BRANCH} --depth 1
  /build/buildmt/BridgePoint/Launcher.sh -nosplash -data wse -application org.eclipse.cdt.managedbuilder.core.headlessbuild -importAll bptest/src
  /build/buildmt/BridgePoint/CLI.sh Build -project org.xtuml.bp.core.test -prebuildOnly
fi
</command>
          </hudson.tasks.Shell>
          <maven-builder>
            <disabled>false</disabled>
            <description>Build product (also builds tests if the INCLUDE_TESTS is true)</description>
            <config>
              <installationId>(Bundled)</installationId>
              <goals>install</goals>
              <privateRepository>true</privateRepository>
              <privateTmpdir>true</privateTmpdir>
              <pomFile>bridgepoint/releng/org.xtuml.bp.releng.parent/pom.xml</pomFile>
              <properties>
                <entries>
                  <entry name="tycho.disableP2Mirrors" value="true"/>
                  <entry name="skipTests" value="!${INCLUDE_TESTS}"/>
                  <entry name="maven.test.failure.ignore" value="${INCLUDE_TESTS}"/>
                  <entry name="eclipse.home" value="/build/buildmt/BridgePoint"/>
                </entries>
              </properties>
              <errors>false</errors>
              <verbosity>NORMAL</verbosity>
              <offline>false</offline>
              <snapshotUpdateMode>NORMAL</snapshotUpdateMode>
              <recursive>true</recursive>
              <checksumMode>NORMAL</checksumMode>
              <failMode>NORMAL</failMode>
              <makeMode>NONE</makeMode>
              <settingsId>NONE</settingsId>
              <globalSettingsId>NONE</globalSettingsId>
              <toolChainsId>NONE</toolChainsId>
            </config>
          </maven-builder>
          <hudson.tasks.Shell>
            <disabled>false</disabled>
            <description>Unit Test - Collect Results</description>
            <command>#!/bin/bash
if [ &quot;$INCLUDE_TESTS&quot; == &quot;true&quot; ]; then
  export MAVEN_OPTS=&quot;-Xmx2g&quot;
  orig_dir=$(pwd)
  cd bridgepoint/releng/org.xtuml.bp.releng.parent
  mvn -Daggregate=true surefire-report:report-only
  cd $orig_dir
fi</command>
          </hudson.tasks.Shell>
          <hudson.tasks.Shell>
            <disabled>false</disabled>
            <description>MASL Roundtrip Testing</description>
            <command>#!/bin/bash
if [ &quot;$INCLUDE_TESTS&quot; == &quot;true&quot; ]; then
  /build/buildmt/roundtrip/roundtrip.sh
fi
</command>
          </hudson.tasks.Shell>
        </originalValue>
        <propertyOverridden>false</propertyOverridden>
      </describable-list-property>
